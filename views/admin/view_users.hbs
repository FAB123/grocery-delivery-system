<script src="https://unpkg.com/ionicons@5.2.3/dist/ionicons.js"></script>
{{>admin-sidebar}}
<section class="content">
    <div class="container-fluid">
        <div class="row mt-2">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">List of all Users</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table id="example1" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>First Name</th>
                                    <th>Last Name</th>
                                    <th>Phone</th>
                                    <th>Username</th>
                                    {{!-- <th>Active</th> --}}
                                </tr>
                            </thead>
                            <tbody>
                                {{#each users}}
                                <tr>
                                    <td>{{this.first_name}}</td>
                                    <td>{{this.last_name}}</td>
                                    <td>{{this.mobile}}</td>
                                    <td> {{this.username}}</td>
                                    {{!-- <td class="d-inline"><input type="checkbox" name="usercontroler" id="{{this._id}}" {{#if this.active}} checked {{/if}}data-bootstrap-switch data-on-text="Active"
                    data-off-text="Blocked" data-off-color="danger" data-on-color="success"></td> --}}
                                </tr>

                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
            </div>
        </div>
    </div>
</section>
</div>